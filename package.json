{
  "name": "@henryhawke/mcp-titan",
  "version": "0.1.4",
  "description": "A Model Context Protocol server with enhanced Titan Memory implementation",
  "type": "module",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "bin": {
    "mcp-titan": "./build/index.js"
  },
  "files": [
    "build",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node build/index.js",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "clean": "rimraf build",
    "prebuild": "npm run clean",
    "prepare": "npm run build",
    "postinstall": "npm run build",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepack": "npm run build"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.4.1",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-node": "^4.22.0",
    "express": "^4.21.2",
    "body-parser": "^1.20.3"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.1",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.3",
    "eslint": "^9.19.0",
    "prettier": "^3.4.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "memory",
    "ai",
    "machine-learning",
    "cursor",
    "claude"
  ],
  "author": "Henry Mayo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/henryhawke/mcp-titan.git"
  },
  "bugs": {
    "url": "https://github.com/henryhawke/mcp-titan/issues"
  },
  "homepage": "https://github.com/henryhawke/mcp-titan#readme"
}